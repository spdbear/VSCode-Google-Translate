{
  "name": "engshaper",
  "displayName": "engshaper",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/spdbear/engshaper"
  },
  "version": "0.0.1",
  "publisher": "spdbear",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.shape",
    "onCommand:extension.shapeAndTranslate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.shape",
        "title": "engshaper: Shape Text"
      },
      {
        "command": "extension.shapeAndTranslate",
        "title": "engshaper: Shape & Translate"
      }
    ],
    "keybindings": [
      {
        "command": "extension.shape",
        "key": "ctrl+s ctrl+p"
      },
      {
        "command": "extension.shapeAndTranslate",
        "key": "ctrl+s ctrl+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "generator-code": "^1.2.7",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vsce": "^1.68.0",
    "vscode-test": "^1.2.0",
    "yo": "^3.1.0"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^3.0.0",
    "request": "^2.88.0"
  }
}
